@article{Labreche2008b,
  title = {Marie-Sissi Labrèche par elle-même},
  volume = {49},
  rights = {Copyright Rogers Publishing Limited May 2008},
  issn = {03172635},
  url = {http://search.proquest.com/docview/217134860/abstract/2D9B0EFC42524A06PQ/3},
  abstract = {La première fois que j'ai visité ce qui allait devenir ma maison. Ce loft me ressemble. Il a de grandes fenêtres, grandes comme mes yeux, il est chaleureux. Et plein de miroirs pour me rappeler qu'il faut toujours s'affronter pour être bien. Aussi, ce loft est l'aboutissement de quelque chose que j'ai toujours cherché : un toit à moi. Quand j'étais enfant, j'écrivais mon nom - Labrèche - avec un accent circonflexe, comme un petit toit.
D'avoir à réexpliquer constamment le principe de l'autofiction, qu'il y a là du vrai et du pas vrai, que c'est moi sans être moi. Mais bon. C'est ma faute après tout, c'est moi qui suis tombée dans le piège. Alors j'assume.
Uavalée des avalés, de Réjean Ducharme. Ce livre m'a appris non seulement à écrire, mais à vivre. Je dis parfois que Réjean Ducharme a agi sur moi comme une Martha Stewart de l'âme avec ses petites comptines trash. En le lisant, j'ai compris toute la distance qu'il fallait que je prenne pour être bien dans ma peau. Pour moi, la littérature et ma vie ont toujours été très collées.},
  timestamp = {2016-12-04T07:16:59Z},
  langid = {french},
  number = {5},
  journaltitle = {Châtelaine},
  author = {Labrèche, Marie-Sissi},
  date = {2008},
  pages = {117--118},
  keywords = {labreche}
}

